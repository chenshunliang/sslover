.search{
  padding: 10px;
  height: 10vh;
  position: absolute;
  top: 0;
  z-index: 100;
  width: 96%;
  margin: auto;
  left: 0;
  right: 0;
}

.search_text{
  border: 1px solid #efefef;
  border-radius: 3px;
  height: 45px;
  line-height: 45px;
  padding: 5px;
  color: gray;
  background-color: white;
}

.text_box{
  padding:0 5px;
  color:gray;
  height:40px;
  line-height:40px;
  font-size:16px;
  border-bottom:1px solid #efefef;
  margin:0 15px;
}

.map{
  width: 100%;
  height: 100vh;
  z-index: 10;
}

.map map{
  width: 100%;
  height: 100%;
}

.key_word{
  position: absolute;
  top: 10vh;
  z-index: 100;
  width: 86%;
  background-color: white;
  left: 0;
  right: 0;
  margin: auto;
}

.tips{
  margin-bottom:5px;
  border-bottom:none;
  font-size:14px;
  font-weight:bold;
  color:#fa5756;
}